<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button001"
        android:layout_width="148dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_marginEnd="132dp"
        android:background="#0095B6"
        android:text="Explicit"
        android:textSize="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_goneMarginTop="204dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="265dp" />

    <Button
        android:id="@+id/button002"
        android:layout_width="133dp"
        android:layout_height="53dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="100dp"
        android:background="#008260"
        android:text="Implicit"
        app:layout_constraintBottom_toBottomOf="@id/button001"
        app:layout_constraintEnd_toEndOf="@id/button001"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/button001" />
</androidx.constraintlayout.widget.ConstraintLayout>